  }

  .contact-form__success.toast {
    position: fixed;
    right: 0;
    background: white;
    padding: 20px;
    bottom: 100px;
    z-index: 99999;
    box-shadow: 0px 1px 11px 0px rgba(0,0,0,0.1);
    -webkit-box-shadow: 0px 1px 11px 0px rgba(0,0,0,0.1);
