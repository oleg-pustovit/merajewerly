    <div class="form-wrapper">
      
  <div class="contact-form__content-blocks">
    {% if settings.first_block_subtitle or settings.first_block_title %}
    <div class="contact-form__content-block" style="background: {{ settings.first_block_bg }}">
      {% if settings.first_block_subtitle %}
        <p class="subtitle">
        {{ settings.first_block_subtitle }}
        </p>
      {% endif %}
       {% if settings.first_block_title %}
        <h2 class="copyable-title">
          {{ settings.first_block_title }}
        </h2>
      {% endif %}
    </div>
    {% endif %}


   {% if settings.second_block_subtitle or settings.second_block_title %}
    <div class="contact-form__content-block" style="background: {{ settings.second_block_bg }}">
      {% if settings.second_block_subtitle %}
