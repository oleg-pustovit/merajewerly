             on:click="/closeDialog"
             disabled
             type="button"
             class="button btn-primary"
            aria-label="{{ 'accessibility.close_dialog' | t }}"
            >
              {{ 'blocks.contact_form.btn_popup_apply' | t }}
            </button>
          </div>
          <button
            ref="closeButton"
            on:click="/closeDialog"
             type="button"
            class="button popup-link__close"
            aria-label="{{ 'accessibility.close_dialog' | t }}"
          >
            {{- 'icon-close.svg' | inline_asset_content -}}
          </button>
        </dialog>
      </dialog-component>
