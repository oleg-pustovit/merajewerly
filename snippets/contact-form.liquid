  }


  .popup-link__inner .title {
    margin-bottom: 30px;
  }

  .contact-form-wrapper.two-column {
    display: grid;
    grid-template-columns: 1fr 0.7fr;
    gap: 80px;
  }
  .contact-form__content-block h2 { 
    font-size: 24px;
    text-transform: none;
  }
  .inquiry-btn span {
    color: #161616;
  }
  .inquiry-btn span.gray {
    color: rgba(22,22,22,0.45)
  }
  @media(max-width: 1300px) {
    .contact-form-wrapper.two-column {
      gap: 40px;
      grid-template-columns: 1fr 0.5fr;
    }
  }
  @media(max-width: 1168px) {
    .contact-form__content-block h2 {
      font-size: 20px;
    }
     .contact-form-wrapper.two-column {
      grid-template-columns: 1fr;
    }
    .image-wrapper {
      display: none;
    }
  }
  @media(max-width: 749px) {
    .contact-form__content-block .subtitle {
      font-size: 14px;
    }
    .button.popup-link__close {
      top: 12px;
    }
    .button.popup-link__close svg {
      width: 16px;
      height: 16px;
      
    }
    .contact-form__content-blocks {
      grid-template-columns: 1fr;
      gap: 8px;
      margin-bottom: 24px;
    }
    .inquiry-blocks {
      grid-template-columns: 1fr 1fr;
      gap: 8px;
    }
    .popup-link__content.dialog-modal {
      padding: 24px 16px;
    }
    .popup-link__inner .title {
      text-align: left;
      font-weight: 500;
      font-size: 24px;
      line-height: 100%;
      letter-spacing: -1%;

    }
  }
</style>


