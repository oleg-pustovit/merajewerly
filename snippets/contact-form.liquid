      <input
        type="text"
        id="ContactForm-inquiry"
        class="contact-form__input hide hidden"
        autocomplete="inquiry"
        name="contact[inquiry]"
        spellcheck="false"
        autocapitalize="off"
        value=""
        aria-required="true"
        {% if form.errors contains 'email' %}
          aria-invalid="true"
          aria-describedby="ContactForm-inquiry-error"
        {% endif %}
        placeholder="{{ 'blocks.contact_form.inquiry' | t }}"
      >

    <label
      class="visually-hidden"
      for="ContactForm-body"
    >
      {{- 'blocks.contact_form.comment' | t -}}
    </label>
    <textarea
      rows="10"
      id="ContactForm-body"
      class="contact-form__input contact-form__input--textarea"
      name="contact[body]"
      placeholder="{{ 'blocks.contact_form.comment' | t }}"
    >
      {{- form.body -}}
    </textarea>

    {{ submit_button }}

    {%- if form.errors -%}
      <div
        class="contact-form__error"
        tabindex="-1"
        autofocus
      >
        {{- 'icon-error.svg' | inline_asset_content -}}

        {{- form.errors.translated_fields.email | capitalize }}
        {{ form.errors.messages.email -}}
      </div>
    {%- endif -%}
    
    {%- if form.posted_successfully? -%}
      <div
        class="contact-form__success toast"
        tabindex="-1"
        autofocus
      >
        <svg width="20" height="21" viewBox="0 0 20 21" fill="none" xmlns="http://www.w3.org/2000/svg">
        <rect x="0.7" y="1.2" width="18.6" height="18.6" rx="9.3" stroke="#7FAC66" stroke-width="1.4"/>
