        {{ settings.second_block_subtitle }}
        </p>
      {% endif %}
       {% if settings.second_block_title %}
        <h2 class="copyable-title">
          {{ settings.second_block_title }}
        </h2>
      {% endif %}
    </div>
    {% endif %}
  </div>
<script>
document.addEventListener('DOMContentLoaded', function() {
  const copyableTitles = document.querySelectorAll('.copyable-title');
  const animationDuration = 600; 

  copyableTitles.forEach(title => {
    const originalText = title.innerText;
    title.dataset.originalText = originalText;

    title.addEventListener('click', function(event) {
      const element = event.currentTarget;
      if (element.classList.contains('is-animating')) {
        return; 
      }
      if (element.innerText == 'Copied!') {
        return;
      }

      const textToCopy = element.dataset.originalText;

      navigator.clipboard.writeText(textToCopy).then(() => {
        element.classList.add('is-animating');
        
        setTimeout(() => {
          element.innerText = 'Copied!';
        }, animationDuration / 2);

        element.addEventListener('animationend', () => {
          element.classList.remove('is-animating');
        }, { once: true }); 

        setTimeout(() => {
          element.classList.add('is-animating');

          setTimeout(() => {
            element.innerText = element.dataset.originalText;
          }, animationDuration / 2);

          element.addEventListener('animationend', () => {
            element.classList.remove('is-animating');
          }, { once: true });

        }, 2000);

      }).catch(err => {
        console.error('Error: ', err);
      });
    });
  });
});
document.addEventListener('DOMContentLoaded', () => {
  // --- Знаходимо всі елементи ---
  const inquiryBlocks = document.querySelectorAll('.inquiry-block');
  const hiddenInput = document.getElementById('ContactForm-inquiry');
  const btn = document.querySelector('.inquiry-btn span');
  const applyButton = document.getElementById('ApplyInquiryButton');

  // --- Перевіряємо, чи всі елементи на місці ---
  if (!inquiryBlocks.length || !hiddenInput || !applyButton) {
    console.warn('Один або більше елементів для вибору теми не знайдено.');
    return;
  }

  // --- Логіка вибору блоку ---
  inquiryBlocks.forEach(block => {
    block.addEventListener('click', function() {
      // `this` посилається на блок, на який клікнули
      const clickedBlock = this;

      // Знімаємо клас 'selected' з усіх блоків
      inquiryBlocks.forEach(b => {
        b.classList.remove('selected');
      });

      // Додаємо клас 'selected' тільки до натиснутого блоку
      clickedBlock.classList.add('selected');

      // ВАЛІДАЦІЯ: робимо кнопку "Apply" активною
      applyButton.disabled = false;
    });
  });

  // --- Логіка кнопки "Apply" ---
  applyButton.addEventListener('click', function() {
    // Знаходимо блок, який наразі має клас 'selected'
    const selectedBlock = document.querySelector('.inquiry-block.selected');

    if (selectedBlock) {
      // Знаходимо текст сабтайтлу всередині обраного блоку
      const subtitleElement = selectedBlock.querySelector('.subtitle');
      
      if (subtitleElement) {
        const subtitleText = subtitleElement.innerText.trim();
        
        // Встановлюємо цей текст як значення нашого прихованого input
        hiddenInput.value = subtitleText;
        btn.innerHTML = subtitleText;
        btn.classList.remove('gray');
        console.log(`Обрана тема: "${subtitleText}" додана в input.`);

        // Тут ви можете додати код для закриття модального вікна, наприклад:
        // document.querySelector('dialog-component').closeDialog();
        // або
        // this.closest('dialog').close();
      }
    } else {
      // Цей блок спрацює, якщо кнопка якось стала активною без вибору
      console.warn('Кнопка "Apply" натиснута, але жоден блок не обрано.');
    }
  });
});
          
</script>

